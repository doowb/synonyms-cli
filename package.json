{
  "name": "synonyms-cli",
  "description": "CLI for looking up synonyms for given words.",
  "version": "1.0.0",
  "homepage": "https://github.com/doowb/synonyms-cli",
  "author": "Brian Woodward (https://doowb.com)",
  "repository": "doowb/synonyms-cli",
  "bugs": {
    "url": "https://github.com/doowb/synonyms-cli/issues"
  },
  "license": "MIT",
  "files": [
    "cli.js",
    "index.js"
  ],
  "main": "index.js",
  "bin": {
    "synonyms": "cli.js",
    "synonyms-cli": "cli.js"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "ansi-bold": "^0.1.1",
    "ansi-cyan": "^0.1.1",
    "ansi-gray": "^0.1.1",
    "longest": "^2.0.1",
    "minimist": "^1.2.0",
    "repeat-string": "^1.6.1",
    "synonyms": "^1.0.1",
    "word-wrap": "^1.2.3"
  },
  "devDependencies": {
    "gulp-format-md": "^1.0.0"
  },
  "keywords": [
    "cli",
    "synonyms"
  ],
  "verb": {
    "toc": false,
    "layout": "global",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
